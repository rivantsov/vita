{
  "ServerTypeForTestExplorer": "Oracle",
  "useBatchMode": "true",
  "ServerTypesForConsoleRun": "MsSql,MySql,Postgres,Oracle,SQLite",
  // Connection strings for different server types
  "MsSqlConnectionString": "Data Source=.;Initial Catalog=VitaTest;Integrated Security=True",
  "MySqlConnectionString": "Server=localhost; Uid=root; Pwd=_pass; Old Guids=true;SslMode=none",
  // Note: for Postgres with windows auth, set Host to IP address, not localhost; you need to create Login Role in PG server matching your user name, pwd does not matter
  // or just create testuser
  "PostgresConnectionString": "Host=127.0.0.1;Port=5432;Database=VitaTest;Pooling=true;userid=postgres;password=_pass",
  "OracleConnectionString": "DBA PRIVILEGE=SYSDBA;USER ID=SYS;Password=_pass4;DATA SOURCE=192.168.0.30:1521/XEPDB1;PERSIST SECURITY INFO=True",
  //"OracleConnectionString": "Data Source=(DESCRIPTION = (ADDRESS=(PROTOCOL = TCP)(HOST =192.168.0.30)(PORT = 1521))(CONNECT_DATA =(SID = CLRExtProc))); User Id=sys;Password=_pass4",

  // SQLite: we can use either provider in this test
  "UseMsSqliteProvider": "false",
  // For SQLite provider
  "SQLiteConnectionString": "Data Source={bin}\\VitaTestSQLite.db;Mode=ReadWriteCreate;foreign keys=true; pooling=true",

  // For MS SQLite provider
  "SQLiteConnectionString_MS": "Data Source={bin}\\VitaTestSQLite.db;Mode=ReadWriteCreate"

}    

