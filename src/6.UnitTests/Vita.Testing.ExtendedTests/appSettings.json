{
  "ServerTypeForTestExplorer": "MySql",
  "UseBatchModeForTestExplorer": "true",
  "ResetDatabase": "true",
  "ServerTypesForConsoleRun": "MsSql,MySql,Postgres,SQLite", //,Oracle",


  // Connection strings for different server types
  "MsSqlConnectionString": "Data Source=.;Initial Catalog=VitaBooks;Integrated Security=True;TrustServerCertificate=True",
  "MySqlConnectionString": "Server=localhost; Uid=root; Pwd=_pass; Old Guids=true;SslMode=none",
  // Note: for Postgres with windows auth, set Host to IP address, not localhost; you need to create Login Role in PG server matching your user name, pwd does not matter
  // or just create testuser
  "PostgresConnectionString": "Host=127.0.0.1;Port=5432;Database=VitaBooks;Pooling=true;userid=postgres;password=_pass",

  "OracleConnectionString": "DBA PRIVILEGE=SYSDBA;Data Source=(DESCRIPTION = (ADDRESS=(PROTOCOL = TCP)(HOST = 192.168.0.100)(PORT = 1521))); User Id=SYS;Password=_pass",

  // For SQLite provider (non-MS)
  "SQLiteConnectionString": "Data Source={bin}\\VitaBooksSQLite.db;Mode=ReadWriteCreate;foreign keys=true; pooling=true"

}    

